package file1;

public class pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,k,a;
		for(i=5,k=0;i>0;i--,k++) {
			for(j=0;j<i;j++) {
				System.out.print(" ");
			}
			for(j=0;j<=k;j++) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
