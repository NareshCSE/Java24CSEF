package java1;


public class Pattern1 {


	public static void main(String[] args) {

		// TODO Auto-generated method stub

		int i,j;

		for(i=1;i<=5;i++) {

			for(j=i;j>0;j--) {

				System.out.print(j);

			}

			for(j=2;j<=i;j++) {

				System.out.print(j);

			}

			System.out.println();

		}


	}


}
