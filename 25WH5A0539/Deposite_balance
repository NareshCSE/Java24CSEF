 class Bank
 {
	public int getBalance()
	 {
		 return 0;
	 }

 }

 class BankA extends Bank
 {
	 public int balance=1000;
	 public int getBalance()
	 {
		 return balance;
	 }
	 
 }
 
 class BankB extends Bank
 {
	 public int balance=1500;
	 public int getBalance()
	 {
		 return balance;
		 
	 }
 }
 
 class BankC extends Bank
 {
	 
	 public int balance=2000;
	 public int getBalance()
	 {
		 return balance;
	 }
	 
 }
 
 

public class Bankbalnce {
		
	public static void main(String[] args) {
		// TODO Auto-generated method stub
     	Bank obj=new BankA();
		Bank obj2=new BankB();
		Bank obj3=new BankC();
		System.out.println("money deposited in bankA:"+obj.getBalance());
		System.out.println("money deposited in bankB:"+obj2.getBalance());
		System.out.println("money deposited in bankC:"+obj3.getBalance());



	}

}
