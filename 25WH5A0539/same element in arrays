package program5;
public class array2 {

	public static void main(String[] args) {
		int[]arr= {10,20,30,40,50};
		int[]arr2= {60,20,10,70,80};
		System.out.println("same elements are");
		for(int i=0;i<arr.length;i++) {
			for(int j=i;j<arr2.length;j++)
			{

				if(arr[i]==arr2[j])
				{
				System.out.println(arr[i]);
			}
		}

	}
 }
}

