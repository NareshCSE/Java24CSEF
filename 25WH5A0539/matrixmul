package program5;
public class matrix {
	public static void main(String[]args)
	{
	  int [][] arr1 ={{10,20,30,40} ,{50,60,70,80}};
	  int [][] arr2 = {{10,20,30,40} , {50,60,70,80}};
	  for(int i=0; i < arr1.length;i++) {
		  for(int j = 0; j < arr2[i].length;j++) {
			 System.out.println(arr1[i][j]*arr2[i][j]);
		  }
	  }
	}
}
		  
	
	
	


