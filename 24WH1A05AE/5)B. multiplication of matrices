package prj1;
import java.util.Scanner;
public class multiplication_matrices {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number of rows for Matrix A: ");
        int row_matrixA = sc.nextInt();
        System.out.print("Enter number of columns for Matrix A: ");
        int col_matrixA = sc.nextInt();
        System.out.print("Enter number of rows for Matrix B: ");
        int row_matrixB = sc.nextInt();
        System.out.print("Enter number of columns for Matrix B: ");
        int col_matrixB = sc.nextInt();
        if (col_matrixA != row_matrixB) {
            System.out.println("Multiplication not possible");
        }
        int[][] A = new int[row_matrixA][col_matrixA];
        int[][] B = new int[row_matrixB][col_matrixB];
        int[][] C = new int[row_matrixA][col_matrixB]; 
        System.out.println("Enter elements of Matrix A:");
        for (int i = 0; i < row_matrixA; i++) {
            for (int j = 0; j < col_matrixA; j++) {
                A[i][j] = sc.nextInt();
            }
        }
        System.out.println("Enter elements of Matrix B:");
        for (int i = 0; i < row_matrixB; i++) {
            for (int j = 0; j < col_matrixB; j++) {
                B[i][j] = sc.nextInt();
            }
        }
        for (int i = 0; i < row_matrixA; i++) {
            for (int j = 0; j < col_matrixB; j++) {
                C[i][j] = 0; 
                for (int k = 0; k < col_matrixA; k++) {
                    C[i][j] += A[i][k] * B[k][j];
                }
            }
        }

        System.out.println("Resultant Matrix C:");
        for (int i = 0; i < row_matrixA; i++) {
            for (int j = 0; j < col_matrixB2; j++) {
                System.out.print(C[i][j] + " ");
            }
            System.out.println();
        }
    }
}
